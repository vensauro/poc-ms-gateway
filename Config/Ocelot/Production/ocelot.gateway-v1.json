{
    "Routes": [
        {
            "UpstreamHttpMethod": [
                "Get", "Put", "Post", "Delete"
            ],
            "UpstreamPathTemplate": "/gateway/v1/{url}",
            "DownstreamPathTemplate": "/{url}",
            "DownstreamScheme": "$GATEWAY_SCHEME",
            "DownstreamHostAndPorts": [
                {
                    "Host": "$GATEWAY_HOST",
                    "Port": "$GATEWAY_PORT"
                }
            ]
        }
    ]
}